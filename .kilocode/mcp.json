{
  "mcpServers": {
    "playwright": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--init",
        "--pull=always",
        "mcr.microsoft.com/playwright/mcp@sha256:48cfafeaaf213c618fff8d2e311f33b170dd1eab87a351260cdb9c24a785fc50"
      ],
      "alwaysAllow": [
        "browser_wait_for",
        "browser_navigate",
        "browser_close",
        "browser_snapshot",
        "browser_generate_playwright_test"
      ]
    },
    "filesystem": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--mount",
        "type=bind,src=Users/User/Desktop/EmbeddingGemma,dst=/projects",
        "mcp/filesystem",
        "/projects"
      ]
    }
  }
}